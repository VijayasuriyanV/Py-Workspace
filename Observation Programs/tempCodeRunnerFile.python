Basic_Salary = int(input("Enter Basic Salary :"))

DA = int(input("Enter DA :"))
HRA = int(input("Enter HRA : "))

Gross_Salary = Basic_Salary + DA + HRA

print ("\nGross Salary :" , Gross_Salary)

Tax_Deduction=(Gross_Salary * 10/100)
print ("\nTax Deducted:" , Tax_Deduction)
print ("\nGross Salary after Tax",Gross_Salary - Tax_Deduction)